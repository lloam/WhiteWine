<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mao.whitewine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_my"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_my"
        android:supportsRtl="true"
        android:theme="@style/MyActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".IndexActivity" />
        <activity android:name=".UpdateUserActivity" />
        <activity android:name=".wine.addWineActivity" />
        <activity android:name=".wine.UpdateWineActivity"></activity>
    </application>

</manifest>